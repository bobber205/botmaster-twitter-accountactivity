    "coveralls": "^2.11.16",
    "documentation": "^4.0.0-beta.18",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2"
